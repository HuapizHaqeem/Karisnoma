<AnchorPane prefWidth="930.0" styleClass="main-content" stylesheets="@../Style.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EdukasiController">
   <children>
    <AnchorPane>
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="687.0" prefWidth="915.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <HBox alignment="CENTER_LEFT" prefWidth="800.0" spacing="322.0">
                     <Label alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" styleClass="dashboard-header" text="Memahami Imunoterapi untuk Kanker Paru">
                        <font>
                           <Font size="24.0" />
                        </font>
                     </Label>
                  </HBox>
                  <ListView fx:id="slideList" prefHeight="475.0" prefWidth="840.0" styleClass="slideshow-image" />
                  <TextArea fx:id="slideContent" editable="false" prefHeight="45.0" prefWidth="840.0" text="*Harap diperhatikan: Tayangan slide ini merupakan interpretasi visual dan tidak dimaksudkan untuk memberikan, atau menggantikan, nasihat medis dan/atau klinis." wrapText="true" />
