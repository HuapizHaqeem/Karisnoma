                     <Button mnemonicParsing="false" onAction="#handlePrev" styleClass="button" text="Previous" />
                     <Button mnemonicParsing="false" onAction="#handleNext" styleClass="button" text="Next" />
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
